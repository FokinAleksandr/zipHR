{
  "name": "ziphr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:1": "react-native run-ios --simulator='iPhone 8'",
    "ios:2": "react-native run-ios --simulator='iPhone 12 Pro Max'",
    "ios:3": "react-native run-ios --simulator='iPhone SE (1st generation)'",
    "start": "react-native start",
    "prettier-fix": "npx prettier src --write --ignore-unknown",
    "eslint-fix": "npx eslint src --fix --ext .ts,.tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/native": "11.9.3",
    "@emotion/react": "11.9.3",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-mmkv": "^2.4.1",
    "react-native-safe-area-context": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "8.9.0",
    "eslint-plugin-jsx-expressions": "1.3.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.2",
    "metro-react-native-babel-preset": "^0.67.0",
    "node-fetch": "^3.2.6",
    "prettier": "^2.7.1",
    "react-test-renderer": "17.0.2",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
